{
	"name": "@lettucebowler/lettuce-wordle",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host 0.0.0.0 --port 3000",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview --host 0.0.0.0 --port=3000",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss .",
		"pages:dev": "wrangler pages dev --compatibility-date=2024-01-17 --proxy 3000 -- pnpm dev",
		"pages:deploy": "pnpm build && wrangler pages deploy .svelte-kit/cloudflare"
	},
	"devDependencies": {
		"@auth/core": "0.25.0",
		"@auth/sveltekit": "^0.11.0",
		"@cloudflare/workers-types": "^4.20240129.0",
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tanstack/svelte-query": "^5.18.0",
		"@types/cookie": "^0.6.0",
		"@types/js-cookie": "^3.0.6",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"autoprefixer": "^10.4.17",
		"cookie": "^0.6.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"itty-fetcher": "^0.9.4",
		"js-cookie": "^3.0.5",
		"postcss": "^8.4.33",
		"prettier": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"svelte": "4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-french-toast": "^1.2.0",
		"svelte2tsx": "^0.7.0",
		"tailwind-scrollbar": "^3.0.5",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"valibot": "^0.27.1",
		"vite": "^5.0.12",
		"wrangler": "^3.26.0"
	},
	"type": "module"
}
